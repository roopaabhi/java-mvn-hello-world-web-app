pipeline {
  agent { label 'slave1'}
    stages {
        stage('Directory') {
            steps {
                sh ' git clone https://github.com/roopaabhi/java-mvn-hello-world-web-app'
            }
        }
        stage('Compile') {
            steps {
                sh 'mvn compile'
            }
        }
        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
}
}

