pipeline {
  agent any
    stages {
        stage('Directory') {
            steps {
                sh 'cd /home/slave1/workspace/pipelinejob/java-mvn-hello-world-web-app/'
            }
        }
        stage('Compile') {
            steps {
                sh 'mvn compile'
            }
        }
        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
}
}

